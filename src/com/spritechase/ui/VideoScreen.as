package com.spritechase.ui{	import flash.text.TextField;		import com.spritechase.core.*;		public class VideoScreen extends UIScreen	{				//Stage members		public var modelName_txt:TextField;				//Private members		private var index:int = -1;		private var data:XMLList;				public function VideoScreen()		{			trace('VideoScreen()');		}				override public function update(newData:KioskData):void		{			data = newData.data..section.(@id == 'video');			index = -1;			nextModel();		}				private function nextModel():void		{			trace('nextModel()');			index++;			if(index > data..model.length()) index = 0;			showModel(data..model[index].@id);		}				private function prevModel():void		{			index--;			if(index < 0) index = data..models.length() - 1;			showModel(data..model[index].@id);		}				private function showModel(id:String):void		{			//$$wtf, where is my null obj ref coming from?			trace('!!!!!!!!! id: ' + id);			trace(', KioskGlobals.data[id]: ' + KioskGlobals.data[id]);						modelName_txt.text = KioskGlobals.data[id].name;			dispatchEvent(new KioskEvent(KioskEvent.NEW_MODEL_SELECTED, {"id": id }));		}				override public function idle():void		{			modelName_txt.text = "...";			dispatchEvent(new KioskEvent(KioskEvent.IDLE));		}	}}