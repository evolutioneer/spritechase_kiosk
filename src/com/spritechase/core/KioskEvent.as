package com.spritechase.core{		import flash.events.Event;		public class KioskEvent extends Event	{				//Event names		public static const NEW_MODEL_SELECTED:String = "newModelSelected";		public static const IDLE:String = "kioskIdle";		public static const CLEAN_QR_FOUND:String = "cleanQrFound";		public static const QR_CANDIDATE_FOUND:String = "qrCandidateFound";		public static const HANDSHAKE_ATTEMPT:String = "handshakeAttempt";		public static const USER_AR_MOVEMENT:String = "userARMovement";				public var data:Object;				public function KioskEvent(type:String, data:Object = null, bubbles:Boolean = true, cancelable:Boolean = false)		{			this.data = data;			super(type, bubbles, cancelable);		}	}}